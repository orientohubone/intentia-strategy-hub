{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/status-rss",
      "destination": "https://vofizgftwxgyosjrwcqy.supabase.co/functions/v1/status-rss"
    },
    {
      "source": "/api/status-webhook",
      "destination": "https://vofizgftwxgyosjrwcqy.supabase.co/functions/v1/status-webhook"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
